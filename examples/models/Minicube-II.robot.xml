      </Geom>
      <Geom type="trimesh">
        <Render>Y1-body.iv 0.001</Render>
        <data>Y1-body.iv 0.001</data>
        <translation>0 0.018 0</translation>
      </Geom>
   </Body>

   <Body name="Head" type="dynamic">
      <translation>0 0.072 0</translation>
      <mass type="sphere">
        <total>0.025</total>
        <radius>0.018</radius>
      </mass>
      <Geom type="trimesh">
        <Render>Y1-head.iv 0.001</Render>
        <data>Y1-head.iv 0.001</data>
        <translation>0 0.054 0</translation>
      </Geom>
    </Body>

   <Joint type="hinge" name="J1">
      <Body>Tail</Body>
      <Body>Seg1</Body>
      <axis>1 0 0</axis>
      <anchor>0 -0.036 0.007</anchor>
      <maxtorque>0.4</maxtorque>
      <maxvel>4.5</maxvel>
      <lostop>-90</lostop>
      <histop>90</histop>
    </Joint>

    <Joint type="hinge" name="J2">
      <Body>Seg1</Body>
      <Body>Seg2</Body>
      <axis>0 0 1</axis>
      <anchor>-0.007 0.036 0</anchor>
      <maxtorque>0.4</maxtorque>
      <maxvel>4.5</maxvel>
      <lostop>-90</lostop>
      <histop>90</histop>
    </Joint>

    <Joint type="hinge" name="J3">
      <Body>Seg2</Body>
      <Body>Head</Body>
      <axis>1 0 0</axis>
      <anchor>0 0.108 0.007</anchor>
      <maxtorque>0.4</maxtorque>
      <maxvel>4.5</maxvel>
      <lostop>-90</lostop>
      <histop>90</histop>
